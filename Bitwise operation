/*Objective - To implement bitwise operation.

Author name - Sandhya V
Created on - 13/12/2019
Dependencies - 
Modified on - 
*/
//preprocessor directives
#include <stdio.h>

int main(void) {
  int a=12,b=10;
  printf("%d %d\n",a,b);
  int c=a&b;
  printf("%d\n",c);
  printf("%d\n",a|b);
  printf("%d\n",a^b);
  printf("%d\n",a=~a);
  printf("%d\n",b=~b);
  printf("2's compliment of a is %d",-(~(~a)+1));
   return 0;
}
