//preprocessor directives
#include <stdio.h>
int a=10,b=10;
int sum(int *a,int*b)
{
 return *a+*b;
}
int main(void) {
  
  printf("\nValue of a and b is %d %d\n",a,b);
  printf("\nAddress of variable a and b are\n %p\n %p\n",&a,&b);
  printf("Sum is %d",sum(&a,&b));
  return 0;
}
