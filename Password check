//preprocessor directive
#include <stdio.h>
int main(void) {
  int password=1234,ippassword;
  Label1:
  printf("Please enter password\n");
  scanf("%d",&ippassword);
  if(ippassword==password)
  {
    printf("Correct password\n");
  }
  else
  {
    printf("Incorrect password\n");
    goto Label1;
  }
 return 0;
}
